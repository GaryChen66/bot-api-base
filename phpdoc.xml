<?xml version="1.0" encoding="UTF-8"?>
<phpdoc>
  <title><![CDATA[Telegram Bot Api Base]]></title>
  <parser>
    <target>docs/cache</target>
    <extensions>
      <extension>php</extension>
    </extensions>
  </parser>
  <transformer>
    <target>docs/api</target>
  </transformer>
  <transformations>
    <template name="responsive" />
  </transformations>
  <logging>
    <level>warn</level>
    <paths>
      <default>{APP_ROOT}/docs/log/{DATE}.log</default>
      <errors>{APP_ROOT}/docs/log/{DATE}.errors.log</errors>
    </paths>
  </logging>
  <files>
    <directory>./src</directory>
  </files>
</phpdoc>